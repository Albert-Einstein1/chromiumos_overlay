# Setting up things manually here lets us avoid HAL overhead.
Section "ServerFlags"
    Option "AutoAddDevices" "false"
    Option "DontZap" "false"
EndSection

# TODO: ST1Q specific keyboard and mouse.

Section "InputDevice"
    Identifier "USBMouse"
    Driver     "mouse"
    Option     "Device"        "/dev/input/mice"
    Option     "Protocol"      "IMPS/2"
    Option     "ZAxisMapping"  "4 5"
EndSection

Section "Device"
    Identifier "MSM"
    Driver     "msm"
    Option     "fb"        "/dev/fb0"
    Option     "DRI2"      "true"
    Option     "Fastfill"  "false"
EndSection

